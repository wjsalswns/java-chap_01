package chap_01;

public class _Quiz_01 {
	public static void main(String[] args) {
		// 버스 번호 (1234, 성남88)
		// ex)number -> no
		// ex) count -> cnt
		// 남은 시간 (3분, 5분) - 정수형
		
		// 남은 거리 (1.5km, 0.8km) - 실수형
		
		// 결과 출력
		// 성남 08번 버스
		// 약 3분 후 도착
		// 남은 거리 1.2km
		
		
		String bus_name = "성남 08 버스";
		int minute = 3;
		double distanace = 1.2;
		System.out.println(bus_name);
		System.out.println("약 " + minute + "분 후 도착");
		System.out.println("남은 거리 약 " + distanace + "km");
	}
}
